header
    .header-wrap
        .container.header-container
            .mobile-header-content
                .mobile-login.mobile-icons-head(data-toggle="modal" data-target="#popup-login")
                    +icon('icon-login')
                .mobile-login-active
                    a(href="/")
                        img.mobile-login-active__img(src="static/img/general/seller.png")
                .mobile-logo
                    +icon('logo')
                .mobile-sandwich.mobile-icons-head
                    +icon('icon-menu')
            .col-12.col-sm-12.col-md-12.header-main-contents-region
                .header-mobile-choise-region
                    .header-mobile-choise-region-text
                        span Регион:
                        span.mobile-region-choise Wien
                    a.header-light-font-blue(href="/") Region ändern
            .col-xl-12.mobile-small-search
                form.mobile-serach-form
                    input.mobile-search-input(type="text" placeholder="Введите текст объявления")
                    button.mobile-search-button(type="sumbit")
                        +icon('search')
                .mobile-category-filters
                    .mobile-category.mobile-buttons-mobile
                        +icon('icon-categories')
                        span Kategorie
            .header-content
                .col-md-5.col-lg-5.col-xl-5.header-advert-region
                    a.col-xl-7.willhaben-button(href="/") Anzeige aufgeben
                    .col-xl-5.header-region-choise
                        span Wien
                        a(href="/") Region ändern
                .col-md-7.col-lg-7.col-xl-7.header-logo-register
                    .logo-wrap
                        +icon('logo')
                    .header-login-reg(data-toggle='modal', data-target='#popup-login') Einloggen | Registrieren
                    .header-2-block.auth-profile
                        .auth-profile-info
                            span Alexander Voss
                            a(href="/") Mein Profil
                        img.auth-profile__logo(src="static/img/general/seller.png")
            include ../modules/menu
            form.header-search-content
                .col-lg-9.col-xl-10.header-search
                    select.test
                        option(value="Вена") Wien
                        option(value="Москва") Moskow
                        option(value="СанктПитербург") Санкт-Питербург
                    .header-search-and-category
                        .header-category-button
                            +icon('catalog')
                            span Kategorie
                    input.inputs(type="text" placeholder="Was suchen Sie?")
                    button.header-search__button(type="submit")
                        +icon('search')
                        span Finden
                .col-lg-3.col-xl-2.header-search__only-logo
                    input#only_photo(type="checkbox")
                    label(for="only_photo")
                    span Nur mit Foto
    .popup-menu-catalog
        .container
            .popup-menu-catalog-wrapper
                .col-xl-4.popup-menu-areas
                    ul.popup-menu-catalog--list
                        li.popup-menu-catalog--item.active
