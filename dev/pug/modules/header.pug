                                +icon('dog')
                                span.popup-menu-catalog--item--span Tiere
                        li.popup-menu-catalog--item
                            a.popup-icon-pos(href="/")
                                +icon('komp')
                                span.popup-menu-catalog--item--span Elektronick
                        li.popup-menu-catalog--item
                            a.popup-icon-pos(href="/")
                                +icon('curch')
                                span.popup-menu-catalog--item--span Haushalt
                        li.popup-menu-catalog--item
