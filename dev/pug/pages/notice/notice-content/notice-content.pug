                        label
                            span.advert-color-name.advert-border-f2e2ca Burgunder
                            input.advert-color-check(type="radio" name="color")
                            .advert-color-choise-checked.advert-F2E2CA
                        label
                            span.advert-color-name.advert-border-FFEA2F Gelb
                            input.advert-color-check(type="radio" name="color")
                            .advert-color-choise-checked.advert-FFEA2F
                        label
                            span.advert-color-name.advert-border-FFB82F Orange
                            input.advert-color-check(type="radio" name="color")
